<Page x:Class="DesktopClient.Pages.AdministratorLogin"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="500"
	Title="AdministratorLogin">

    <Grid>
        <Label Content="Введите ваши данные" HorizontalAlignment="Left" VerticalAlignment="Top" Height="30" Width="135" Margin="10,10,0,0"/>
        
        <Label Content="Login" HorizontalAlignment="Left" Margin="10,73,0,0" VerticalAlignment="Top"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="10,104,0,0" VerticalAlignment="Top"/>

        <Button Name="Button_Back" Content="Back" HorizontalAlignment="Left" Margin="10,268,0,0" VerticalAlignment="Top" Width="75" Height="22" />
        <Button Name="Button_Continue" Content="Continue" HorizontalAlignment="Left" Margin="415,268,0,0" VerticalAlignment="Top" Width="75"/>
        
        <TextBox Name="TextBox_Login" HorizontalAlignment="Left" Height="23" Margin="119,73,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="TextBox_Password" HorizontalAlignment="Left" Height="23" Margin="119,104,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Page>
