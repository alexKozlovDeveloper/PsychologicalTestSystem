        <Label FontSize="30" Content="Тест" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="240"/>
        <Label FontSize="30" Content="Вопрос" HorizontalAlignment="Left" Margin="10,65,0,0" VerticalAlignment="Top" Width="240"/>
        <ComboBox FontSize="30" Name="ComboBoxTest" HorizontalAlignment="Left" Margin="296,13,0,0" VerticalAlignment="Top" Width="288"/>
        <ComboBox FontSize="30" Name="ComboBoxQuestion" HorizontalAlignment="Left" Margin="296,68,0,0" VerticalAlignment="Top" Width="288"/>

    </Grid>
</Window>
